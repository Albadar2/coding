#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    int a;
    cin>>n;
     i=1;
     while(i<=n){
        j=1;
        
        while(j<i){
            cout<<" ";
            j++;
        }
        j=1;
         while(j<=n-i+1){
            
         
             cout<<i;
             j++;
             
            }
        
         cout<<endl;
         i++;
         }
    return 0;
}
