#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    char ch;
    cin>>n;
     i=1;
     while(i<=n){
         j=1;
         int s=0;
         while(j<=i){
             ch='A'+n-i+s;
             j++;
             s++;
             cout<<ch;
             
             
            }
         cout<<endl;
         i++;
         }
    return 0;
}
