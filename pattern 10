#include<iostream>
using namespace std;
int main(){
    int n,i,j;
    int space;
   
    char ch;
    cin>>n;
     i=1;
     while(i<=n){
       space=n-i;
     while(space>0){
         cout<<"1";
        space = space-1;}
        j=1;
        while(j<=i){
         cout<<"*";
             j++;
             
             
             
             
            }
         cout<<endl;
         i++;
         }
    return 0;
}
